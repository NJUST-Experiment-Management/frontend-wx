.list{
  padding: 20rpx 20rpx 0rpx 20rpx;
  margin: auto 40rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0 0  20rpx grey;
  display: flex;
  flex-direction: column;
  background-color: rgb(252, 229, 207);
  position: relative;
}
.show{
  text-align: center;
}
.showView{
  display: flex;
  flex-direction: column;
}
.divLine{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
  align-self: center;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
 }
text{
  font-size:.875rem;
}

.day{
  background-color: rgb(96, 108, 221);
  color: wheat;
  box-shadow: 0 0  20rpx grey;
  position: absolute;
  right: 10%;
  top:30%;
  width: 70px;
  height: 70px;
  border-radius: 50%;
  text-align: center;
  line-height: 70px;
}


.courselist{
  margin-top: 20px;
  padding-bottom: 100px ;
}

